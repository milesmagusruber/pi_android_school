<resources>

    <!-- Base application theme. -->
    <style name="Theme.SSFS" parent="Theme.MaterialComponents.DayNight.DarkActionBar"><!--parent="Theme.AppCompat.Light.DarkActionBar">-->
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Theme.SSFS.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.SSFS.AppBarOverlay" parent="ThemeOverlay.MaterialComponents.Dark.ActionBar"/> <!--parent="ThemeOverlay.AppCompat.Dark.ActionBar" />-->

    <style name="Theme.SSFS.PopupOverlay" parent="ThemeOverlay.MaterialComponents.Light"/><!--parent="ThemeOverlay.AppCompat.Light" />-->

    <style name="Theme.SSFS.Launcher">
        <item name="android:windowBackground">@drawable/logo</item>
        <item name="colorPrimaryDark">@android:color/black</item>
    </style>



</resources>
